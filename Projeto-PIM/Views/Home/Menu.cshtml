
@{
    ViewBag.Title = "Menu";

}

<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>GRIMPI - Menu</title>
    <link rel="stylesheet" href="~/Content/CSS/MENU.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">


</head>
<body>
    <header class="header" >
        <nav class="navbar navbar-expand-lg" style="background-color: #2ecc71;">
            <a class="navbar-brand" href="#"></a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNavDropdown">

                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a href="@Url.Action("Sobre2")" class="nav-item">Sobre</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("Carrinho")">Carrinho</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Produtos
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                            <a class="dropdown-item" href="@Url.Action("Produtos")">Cebolinha</a>
                            <a class="dropdown-item" href="@Url.Action("ProdutoCoentro")">Coentro</a>
                            <a class="dropdown-item" href="@Url.Action("ProdutoLavanda")">Lavanda</a>
                            <a class="dropdown-item" href="@Url.Action("ProdutoAlecrim")">Alecrim</a>
                            <a class="dropdown-item" href="@Url.Action("ProdutoHortela")">Hortelã</a>
                            <a class="dropdown-item" href="@Url.Action("ProdutoOregano")">Orégano</a>
                        </div>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="IMG-CARROSSEL" src="~/Content/IMAGE/ervagrimpi.png" alt="First slide">
            </div>
            <div class="carousel-item">
                <img class="IMG-CARROSSEL" src="/Content/IMAGE/grimpiverde.png" alt="Second slide">
            </div>
            <div class="carousel-item">
                <img class="IMG-CARROSSEL" src="/Content/IMAGE/lavandagrimpi.png" alt="Third slide">
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

    
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</body>

</html>
<footer class="rodape">
    <p>© 2024 - Onde a natureza encontra a cidade.</p>
</footer>



